                // Check box functionality
                $("[id*=ContentPlaceHolder1_chNote] input:checkbox").each(function (index, element) {
                    var e = $(element);
                    if (index == ind) {
                        e.parent().css('display', 'none');
                    } else {
                        e.parent().css('display', 'block');
                    }
                });

                $("#ContentPlaceHolder1_chNote").css('display', 'block');

