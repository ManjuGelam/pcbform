        });

        // RadioButton list change event
        $('#ContentPlaceHolder1_RadioComplianceReport input').change(function () {
            $('#ContentPlaceHolder1_RadioComplianceReport').find('input').each(function () {
                if ($(this).prop('checked')) {
                    if (parseInt($(this).prop('value').replace('0', '')) > 1) {
                        $('#ContentPlaceHolder1_rdoOrderissuedby').find('input').each(function () {
                            if (parseInt($(this).prop('value').replace('0', '')) > 3) { $(this).attr('disabled', true); }
                            else { $(this).attr('disabled', false); }
                        });
                    } else {
                        $('#ContentPlaceHolder1_rdoOrderissuedby').find('input').each(function () {
                            if (parseInt($(this).prop('value').replace('0', '')) < 4) { $(this).attr('disabled', true); }
                            else { $(this).attr('disabled', false); }
                        });
                    }
                }
            });
        });

    });
