<!-- Calender js functionality -->
<script type="text/javascript">
    $(document).ready(function () {

        // Calendar Js functionality
        $("#ContentPlaceHolder1_txtOrderDate").datepicker({
            showOtherMonths: true,
            changeMonth: true,
            changeYear: true,
            dateFormat: 'mm/dd/yy'
            //minDate: 0
            //maxDate: 0
            //yearRange: '1900:+0'
        });

        $('.select2').select2();

        if ($('#ContentPlaceHolder1_ddlComplianceReport').val() == "03" || $('#ContentPlaceHolder1_ddlComplianceReport').val() == "02") {
            $('#Others').css('display', 'none');
            if ($('#ContentPlaceHolder1_ddlComplianceReport').val() == "03") {
                $('#CFOCategory').css('display', 'block');
                $('#LineOfAcitvity').css('display', 'none');
            }
            else if ($('#ContentPlaceHolder1_ddlComplianceReport').val() == "02") {
                $('#LineOfAcitvity').css('display', 'block');
                $('#CFOCategory').css('display', 'none');
            }
            $('#CFOContent').css('display', 'block');
        } else {
            $('#Others').css('display', 'block');
            $('#CFOContent').css('display', 'none');
        }

    });

</script>
